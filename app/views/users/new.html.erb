<h1>Create New User</h1>
<h3><p><%= link_to "Sign In", login_path %></p> </h3>
<% @user.errors.each do |attr,msg|  %>
 <%=attr %> <%=msg %>
<% end %>

<%= form_for @user do |f| %>
Username: <%= f.text_field :username %><br>
Password: <%= f.password_field :password %><br>
Email: <%= f.text_field :email %>
<br><br>
<%= f.submit "Sign up" %>
<%end%>
