<h1>Sign In</h1>
<%= link_to('Log in with Github!', '/auth/github') %><br>
<p>New user? <%= link_to "Sign up", signup_path %></p>
<%= form_for :sessions, url: login_path do |f| %>
Username: <%= f.text_field :username %><br>
Password: <%= f.password_field :password %><br>
<br><br>
<%= f.submit "Log In"%>
<%end%>
